{
    "manifests": {
        "eonx-com/easy-bugsnag": {
            "manifest": {
                "bundles": {
                    "EonX\\EasyBugsnag\\Bridge\\Symfony\\EasyBugsnagSymfonyBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                }
            },
            "files": {
                "config/packages/easy_bugsnag.yaml": {
                    "contents": [
                        "easy_bugsnag:",
                        "    api_key: '%env(for_build:BUGSNAG_API_KEY)%'",
                        "",
                        "    app_name:",
                        "        enabled: true",
                        "        env_var: 'AWS_ECS_TASK_NAME'",
                        "",
                        "    aws_ecs_fargate:",
                        "        enabled: true",
                        "        meta_url: '%env(for_build:ECS_CONTAINER_METADATA_URI_V4)%/task'",
                        "",
                        "    session_tracking:",
                        "        enabled: true",
                        "        exclude_urls:",
                        "            - '^/ping'",
                        "        messenger_message_count_for_sessions: true",
                        "",
                        "    worker_info:",
                        "        enabled: true",
                        ""
                    ],
                    "executable": false
                },
                "config/packages/dev/easy_bugsnag.yaml": {
                    "contents": [
                        "easy_bugsnag:",
                        "    aws_ecs_fargate:",
                        "        enabled: false",
                        "",
                        "    session_tracking:",
                        "        enabled: false",
                        ""
                    ],
                    "executable": false
                },
                "config/packages/local/easy_bugsnag.yaml": {
                    "contents": [
                        "easy_bugsnag:",
                        "    aws_ecs_fargate:",
                        "        enabled: false",
                        "",
                        "    session_tracking:",
                        "        enabled: false",
                        ""
                    ],
                    "executable": false
                },
                "config/packages/test/easy_bugsnag.yaml": {
                    "contents": [
                        "easy_bugsnag:",
                        "    aws_ecs_fargate:",
                        "        enabled: false",
                        "",
                        "    session_tracking:",
                        "        enabled: false",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "5646c0901ff00a3ad476bd422c532d32f846a7b3"
        }
    }
}
